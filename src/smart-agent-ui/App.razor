<Router AppAssembly="@typeof(App).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(MudBlazor.MudLayout)" />
    </Found>
    <NotFound>
        <PageTitle>Not found</PageTitle>
        <LayoutView Layout="@typeof(MudBlazor.MudLayout)">
            <MudText Typo="Typo.h4" Class="ma-8">Sorry, there's nothing at this address.</MudText>
        </LayoutView>
    </NotFound>
</Router>